{
  "name": "jsemver",
  "version": "1.1.0",
  "description": "A plain Java object representation of Semantic Versioning",
  "main": "index.js",
  "scripts": {
    "test": "echo \"No test specified\"",
    "prepare": "husky install",
    "prepare-release": "standard-version",
    "prepare-release:minor": "standard-version --release-as minor",
    "prepare-release:patch": "standard-version --release-as patch",
    "prepare-release:major": "standard-version --release-as major"
  },
  "author": "Andrea Maglie",
  "license": "ISC",
  "devDependencies": {
    "husky": "7.0.1",
    "standard-version": "9.3.1"
  },
  "dependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0"
  }
}
